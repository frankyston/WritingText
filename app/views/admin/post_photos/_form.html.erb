<%= simple_form_for(@admin_post_photo) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :photo %>
  </div>

  <div class="form-actions">
  	<%= f.input :admin_post_id, :as => :hidden, :input_html => { :value => params[:post_id] }%>
    <%= f.button :submit %>
  </div>
<% end %>
<br>