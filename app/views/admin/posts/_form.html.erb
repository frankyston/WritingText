<%= simple_form_for(@admin_post) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :resume %>
    <%= f.input :message %>
    <%= f.association :admin_category %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
<br>
